import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ip = new Scanner(System.in);
        int n=ip.nextInt();
        int[] arr = new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=ip.nextInt();
        }
        int x=ip.nextInt();
        for(int i=0;i<n;i++)
        {
            if(arr[i] == x)
            {
                System.out.println(x+" is presented in an array.");
                return;
            }
        }
        System.out.println(x+" is not presented in an array.");
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
